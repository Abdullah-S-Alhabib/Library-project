class books:

    def __init__(self):
        name1 = input("Enter Book name:")
        price1 = input("Enter Book price:")
        YOP1 = input("Enter Book Year of publication:")
        ISBN1 =input("Enter ISBN")
        self.price=price1
        self.YOP=YOP1
        self.ISBN=ISBN1
        self.name=name1



    def get_ISBN(self):
        return self.ISBN
    def set_ISBN(self):
        self.ISBN=input("please enter ISBN:")
    def set_name(self):
        self.name=input("enter name please:")
    def get_name(self):
        return self.name
    def set_price(self):
        self.price=input("Enter price :")
    def get_price(self):
        return self.price
    def set_YOP(self):
        self.YOP=input("please Enter year of publication")
    def get_YOP(self):
        return self.YOP
